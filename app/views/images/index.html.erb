<% content_for :head do %>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <title>ImageBank Browser - Dreamcatcher</title>
  <%= stylesheet_link_tag "imageBank", "imageBank.browse", "imageBank.artist", "imageBank.artistList", 
    "imageBank.slideshow", "imageBank.search", "imageBank.searchList", "imageBank.searchImages" %>
  <%= javascript_include_tag "functions.core", "compiled/imagebank", "imageBank" %>
  <script type="text/javascript">
    $(document).ready(function(){
      $('#IB_browser_frame').fadeIn();
      loadBrowse();
    });
  </script>

<% end %>

<%= render :partial => 'images/browser/image_browser' %>

</body>
</html>
