/ CONTEXT PANEL
= render :partial => "users/context_panel", :locals => {:user => @user}

/ ENTRY FIELD
#entryField.field

  / PAGE HEADER
  - if @filters[:type]
    .pageHeader
      .img{class: @filters[:type]}
      %h1= @filters[:type].pluralize
    .clear

  = render :partial => "field"

  #showEntry{style: 'display:none'}
  #newEditEntry{style: 'display:none'}

- content_for :foot do
  / APPLIES SELECTED CLASS TO META MENU
  - if @user == current_user
    :coffeescript
      $('.item.home').addClass('selected')

