<div class="entryImageContainer" data-id="<%= this.id %>">
  <div class="entryImage" style="background: url(/images/uploads/<%= this.id %>-thumb.<%= this.format %>) no-repeat scroll center center transparent">
    <div class="close"></div>
    <label class="radio">
      <input type="radio" name="entry[main_image_id]" data-id="<%= this.id %>">
      <span>default</span>
    </label>
  <div class="radioWrap"></div>
  </div>
</div>
<input type="hidden" id="entry_image_ids_" name="entry[image_ids][]" value="<%= this.id %>" class="image_upload">