<% content_for :head do %>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <title>ImageBank Manager - Dreamcatcher</title>
  <%= stylesheet_link_tag "IB_manager", "fileuploader" %>
  <%= javascript_include_tag "fileuploader", "IB_manager" %>
<% end %>

<!-- IB_browser_frame -->
<div id="IB_browser_frame" class="round-12"> 
  
  <!-- IB_top -->
  <div id="IB_top" class="light A-shine-36 round-12 gradient-light"> 
    
    <div id="IB_browseBackWrap" class="gradient-0-40 round-12">
      <div id="IB_browseBack" class="gradient-1-down round-12"></div>
    </div>
    <img id="IB_editManager" src="/images/icons/editManager-37.png" width="37" height="37" />
    <h1 id="IB_category">Manager</h1>
    
    <div id="IB_searchButtonWrap" class="gradient-0-40 round-12">
      <div id="IB_searchButton" class="gradient-0-30 round-12">
        <img src="/images/icons/searchIcon36.png" width="36" height="36" />
      </div>
    </div>

  </div><!-- END IB_top -->

  <!-- IB_dropboxContainer -->
  <div id="IB_dropboxCover">
    <p>Please select a TYPE, CATEGORY & GENRE before uploading.</p>
  </div>
  <div id="IB_dropboxContainer">
    <!-- generated by fileuploader.js -->
  
  </div><!-- END IB_dropboxContainer -->


  <h2 class="IB_managerHeader">Organization</h2>  
  <!-- IB_managerOrganizationContent -->
  <div class="IB_managerOrganizationContent gradient-10-20">
    
    <h3>Type</h3>
    <form action"">
    <div class="IB_managerOrganizationWrap round-12">
      <span id="IB_current_type" class="round-12">Choose</span>
      <input id="IB_type_checkbox" type="checkbox"/>
      <select id="IB_typeList" class="IB_managerOrganizationList">
        <option value="None"></option>
        <option value="Library">Library</option>
        <option value="Bedsheets">Bedsheets</option>
        <option value="Prima Materia">Prima Materia</option>
        <option value="Book Covers">Book Covers</option>
      </select>
    </div>
    
    <h3>Category</h3>
    <div class="IB_managerOrganizationWrap round-12">
      <span id="IB_current_category" class="round-12">Choose</span>
      <input id="IB_category_checkbox" type="checkbox"/>
      <select id="IB_categoryList" class="IB_managerOrganizationList">
        <option value="None"></option>
        <option value="Modern Art">Modern Art</option>
        <option value="Classical Art">Classical Art</option>
        <option value="Photo">Photo</option>
      </select>
    </div>
    
    <h3>Genre</h3>
    <div class="IB_managerOrganizationWrap round-12">
      <span id="IB_current_genre" class="round-12">Choose</span>
      <input id="IB_genre_checkbox" type="checkbox"/>
      <select id="IB_genreList" class="IB_managerOrganizationList">
        <option value="None"></option>
      </select>
    </div>
    </form>
    
  </div><!-- END IB_managerOrganizationContent -->

  
  <h2 class="IB_managerHeader">Info</h2>
  <!-- IB_managerInfoContent -->  
  <div class="IB_managerInfoContent gradient-10-20">  
    
    <div class="IB_managerInfo">
      <h3>Title</h3>
      <div class="IB_managerInfoInputWrap gradient-light-up round-8">
        <input id="IB_title_input" class="IB_managerInfoInput meta-value" type="text"/>
        <input id="IB_title_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Album</h3>
      <div class="IB_managerInfoInputWrap gradient-light-up round-8">
        <input id="IB_album_input" class="IB_managerInfoInput meta-value" type="text"/>
        <input id="IB_album_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Author</h3>
      <div class="IB_managerInfoInputWrap gradient-light-up round-8">
        <input id="IB_author_input" class="IB_managerInfoInput meta-value" type="text"/>
        <input id="IB_author_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Location</h3>
      <div class="IB_managerInfoInputWrap gradient-light-up round-8">
        <input id="IB_location_input" class="IB_managerInfoInput meta-value" type="text"/>
        <input id="IB_location_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Year</h3>
      <div class="IB_managerInfoInputWrapYear gradient-light-up round-8">
        <input id="IB_year_input" class="IB_managerInfoInputYear meta-value" type="text"/>
        <input id="IB_year_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrapYear -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Notes</h3>
      <div class="IB_managerInfoInputWrapNotes gradient-light-up round-8">
        <input id="IB_notes_input" class="IB_managerInfoInputNotes meta-value" type="text"/>
        <input id="IB_notes_checkbox" class="IB_managerInfoCheckbox" type="checkbox"/>
      </div><!-- End IB_managerInfoInputWrapNotes -->
    </div><!-- End IB_managerInfo -->
        
  </div><!-- End IB_managerInfoContent -->
  

  <h2 class="IB_managerHeader">Source</h2>
  <!-- IB_managerSourceContent -->  
  <div class="IB_managerSourceContent gradient-10-20">
    
    <div class="IB_managerInfo">
      <h3>Date</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_date_input" class="IB_managerSourceInput meta-value" type="text"/>
        <input id="IB_date_checkbox" class="IB_managerSourceCheckbox" type="checkbox"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>User</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_user_input" class="IB_managerSourceInput meta-value" type="text"/>
        <input id="IB_user_checkbox" class="IB_managerSourceCheckbox" type="checkbox"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Geotag</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_geotag_input" class="IB_managerSourceInput meta-value" type="text"/>
        <input id="IB_geotag_checkbox" class="IB_managerSourceCheckbox" type="checkbox"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <br />
    
    <div class="IB_managerInfo">
      <h3>Image Name</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_filename_input" disabled='true' class="IB_managerSourceInput meta-value" type="text"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Resolution</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_resolution_input" disabled='true' class="IB_managerSourceInput meta-value" type="text"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>File Size</h3>
      <div class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_filesize_input" disabled='true' class="IB_managerSourceInput meta-value" type="text"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->
    
    <div class="IB_managerInfo">
      <h3>Source URL</h3>
      <div style="width: 690px" class="IB_managerSourceInputWrap gradient-light-up round-8">
        <input id="IB_source_input" class="IB_managerSourceInput meta-value" style="width: 640px;" type="text"/>
        <input  id="IB_source_checkbox" class="IB_managerSourceCheckbox" type="checkbox"/>
      </div><!-- End IB_managerSourceInputWrap -->
    </div><!-- End IB_managerInfo -->

  </div><!-- END IB_managerSourceContent -->
  
 
  
  
  <h2 class="IB_managerHeader">Tags</h2>
  <!-- IB_managerTagContent -->  
  <div class="IB_managerTagContent gradient-10-20">
    
    <div class="IB_managerTag">
      <div class="IB_managerTagInputWrap gradient-light-up round-8">
        <textarea class="IB_managerTagInput meta-value" type="text" rows="2" cols="75"></textarea>
        <input class="IB_managerTagCheckbox" type="checkbox"/>
      </div><!-- End IB_managerTagInputWrap -->
    </div><!-- End IB_managerTag -->
  
  </div><!-- END IB_managerTagContent -->
  
  <!-- IB_manager Save & Cancel Buttons -->
  <div id="IB_managerSaveWrap" class="gradient-40-0 round-12">
    <div id="IB_managerSave" class="gradient-0-30 round-12">Save</div>
  </div>
  <div id="IB_managerCancelWrap" class="gradient-40-0 round-12">
    <div id="IB_managerCancel" class="gradient-0-30 round-12">Cancel</div>
  </div><!-- END IB_manager Save & Cancel Buttons -->
  <div class="clear"></div>
  
  
</div><!-- END IB_browser_frame -->
