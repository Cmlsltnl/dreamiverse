!!! 5
%html
  %head
    %title Dreamcatcher
    = sass_link_tag 'shared', 'home'
    = javascript_include_tag 'jquery.min', 'pubsub', 'jquery.lightbox-0.5.pack'
    = coffeescript_include_tag 'dreamplugs', 'cookies', 'publicstream'
    =# javascript_include_tag "http://connect.facebook.net/en_US/all.js"
    / %script{type: "text/javascript", :'data-main' => "/javascripts/compiled/application", src: "/javascripts/require.js"}


    = csrf_meta_tag
    = yield :head

  %body.home.light
    -# render "shared/facebook_sdk"

    - unless current_user.blank?
      / META MENU - USER
      = render :partial => "users/meta_menu_user"


    - unless flash.notice.blank? && flash.alert.blank?
      / ALERT PANEL 
      #geoHeader
        %p= flash.notice
        %p= flash.alert


    #drop_shadow
    #home


      = yield

    - if Rails.env == 'production'
      = javascript_include_tag 'google_analytics'

